<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <ThingDef ParentName="PlantBase">
    <defName>Plant_HumanCocoon</defName>
    <label>human meat cocoon</label>
    <description>A pod full of delicious human meat.</description>
    <statBases>
      <MaxHitPoints>85</MaxHitPoints>
      <Nutrition>0</Nutrition>
    </statBases>
    <graphicData>
      <texPath>Plant_HumanCocoon</texPath>
      <graphicClass>Graphic_Random</graphicClass>
    </graphicData>
    <selectable>true</selectable>
    <pathCost>10</pathCost>
    <ingestible>
      <foodType>Plant</foodType>
      <preferability>NeverForNutrition</preferability>
    </ingestible>
    <plant>
      <dieIfLeafless>true</dieIfLeafless>
      <maxMeshCount>4</maxMeshCount>
      <harvestTag>Standard</harvestTag>
      <harvestedThingDef>Meat_Human</harvestedThingDef>
      <harvestYield>70</harvestYield>
      <sowTags>
        <li>Ground</li>
        <li>Hydroponic</li>
      </sowTags>
      <topWindExposure>0.1</topWindExposure>
      <growDays>6.8</growDays>
      <visualSizeRange>0.3~1.0</visualSizeRange>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
</Defs>